<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.YudizApplication" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your light theme here. -->
         <item name="android:statusBarColor">#FFFFFF</item>
    </style>
    <style name="android_button_checkBox">
        <item name="android:button">@drawable/check_box_selector</item>
    </style>
    <style name="Theme.YudizApplication" parent="Theme.MaterialComponents.DayNight.NoActionBar">
